// Stability: Stable module API (vox/lex).
// Migration: Backward-compatible within minor releases; breaking changes require migration notes.

// LexError is a minimal error channel for stage1 lexer.
pub enum LexError {
  None,
  UnexpectedChar(i32),     // byte offset
  UnterminatedString(i32), // byte offset of opening quote
}

pub fn lex_error_to_string(e: LexError) -> String {
  return match e {
    LexError.None => "none",
    LexError.UnexpectedChar(at) => "unexpected char at ".concat(at.to_string()),
    LexError.UnterminatedString(at) => "unterminated string at ".concat(at.to_string()),
  };
}

pub struct LexResult {
  pub tokens: Vec[Token],
  pub err: LexError,
}

fn is_space(b: i32) -> bool {
  return b == 32 || b == 9 || b == 10 || b == 13; // ' ' '\t' '\n' '\r'
}

fn is_alpha(b: i32) -> bool {
  return (b >= 65 && b <= 90) || (b >= 97 && b <= 122) || b == 95; // A-Z a-z _
}

fn is_digit(b: i32) -> bool {
  return b >= 48 && b <= 57; // 0-9
}

fn is_alnum(b: i32) -> bool {
  return is_alpha(b) || is_digit(b);
}

fn is_utf8_cont_byte(b: i32) -> bool {
  return b >= 128 && b <= 191;
}

fn utf8_char_width(head: i32) -> i32 {
  if head >= 0 && head <= 127 { return 1; }
  if head >= 194 && head <= 223 { return 2; }
  if head >= 224 && head <= 239 { return 3; }
  if head >= 240 && head <= 244 { return 4; }
  return 1;
}

// Hot-path byte read used by the lexer after it has done index bounds checks.
fn text_byte_at(text: String, idx: i32) -> i32 {
  let base: rawptr = (text as const rawptr) as isize as rawptr;
  return @ptr_read[u8](@ptr_offset(base, idx as isize)) as i32;
}

fn utf8_has_valid_cont_bytes(text: String, start: i32, width: i32, n: i32) -> bool {
  if width <= 1 { return true; }
  if start + width > n { return false; }
  let mut i: i32 = start + 1;
  while i < (start + width) {
    if !is_utf8_cont_byte(text_byte_at(text, i)) { return false; }
    i = i + 1;
  }
  return true;
}

fn utf8_next_scalar_end(text: String, start: i32, n: i32) -> i32 {
  if start >= n { return n; }
  let width: i32 = utf8_char_width(text_byte_at(text, start));
  if utf8_has_valid_cont_bytes(text, start, width, n) { return start + width; }
  return start + 1;
}

fn kw_kind(text: String, start: i32, end: i32) -> TokenKind {
  // Keyword detection without substring support: compare by length + bytes.
  let n: i32 = end - start;

  // 2
  if n == 2 {
    let b0: i32 = text_byte_at(text, start + 0);
    let b1: i32 = text_byte_at(text, start + 1);
    if b0 == 102 && b1 == 110 { return TokenKind.KwFn; } // fn
    if b0 == 105 && b1 == 102 { return TokenKind.KwIf; } // if
    if b0 == 105 && b1 == 110 { return TokenKind.KwIn; } // in
    if b0 == 97 && b1 == 115 { return TokenKind.KwAs; }  // as
    return TokenKind.Ident;
  }

  // 3
  if n == 3 {
    let b0: i32 = text_byte_at(text, start + 0);
    let b1: i32 = text_byte_at(text, start + 1);
    let b2: i32 = text_byte_at(text, start + 2);
    if b0 == 108 && b1 == 101 && b2 == 116 { return TokenKind.KwLet; } // let
    if b0 == 109 && b1 == 117 && b2 == 116 { return TokenKind.KwMut; } // mut
    if b0 == 112 && b1 == 117 && b2 == 98 { return TokenKind.KwPub; }  // pub
    if b0 == 102 && b1 == 111 && b2 == 114 { return TokenKind.KwFor; }  // for
    if b0 == 116 && b1 == 114 && b2 == 121 { return TokenKind.KwTry; }  // try
    return TokenKind.Ident;
  }

  // 4
  if n == 4 {
    let b0: i32 = text_byte_at(text, start + 0);
    let b1: i32 = text_byte_at(text, start + 1);
    let b2: i32 = text_byte_at(text, start + 2);
    let b3: i32 = text_byte_at(text, start + 3);
    if b0 == 101 && b1 == 108 && b2 == 115 && b3 == 101 { return TokenKind.KwElse; } // else
    if b0 == 101 && b1 == 110 && b2 == 117 && b3 == 109 { return TokenKind.KwEnum; } // enum
    if b0 == 102 && b1 == 114 && b2 == 111 && b3 == 109 { return TokenKind.KwFrom; } // from
    if b0 == 105 && b1 == 109 && b2 == 112 && b3 == 108 { return TokenKind.KwImpl; } // impl
    if b0 == 108 && b1 == 111 && b2 == 111 && b3 == 112 { return TokenKind.KwLoop; } // loop
    if b0 == 116 && b1 == 114 && b2 == 117 && b3 == 101 { return TokenKind.KwTrue; } // true
    if b0 == 116 && b1 == 121 && b2 == 112 && b3 == 101 { return TokenKind.KwType; } // type
    return TokenKind.Ident;
  }

  // 5
  if n == 5 {
    let b0: i32 = text_byte_at(text, start + 0);
    let b1: i32 = text_byte_at(text, start + 1);
    let b2: i32 = text_byte_at(text, start + 2);
    let b3: i32 = text_byte_at(text, start + 3);
    let b4: i32 = text_byte_at(text, start + 4);
    if b0 == 119 && b1 == 104 && b2 == 105 && b3 == 108 && b4 == 101 { return TokenKind.KwWhile; } // while
    if b0 == 109 && b1 == 97 && b2 == 116 && b3 == 99 && b4 == 104 { return TokenKind.KwMatch; }  // match
    if b0 == 98 && b1 == 114 && b2 == 101 && b3 == 97 && b4 == 107 { return TokenKind.KwBreak; }  // break
    if b0 == 119 && b1 == 104 && b2 == 101 && b3 == 114 && b4 == 101 { return TokenKind.KwWhere; } // where
    if b0 == 102 && b1 == 97 && b2 == 108 && b3 == 115 && b4 == 101 { return TokenKind.KwFalse; } // false
    if b0 == 99 && b1 == 111 && b2 == 110 && b3 == 115 && b4 == 116 { return TokenKind.KwConst; } // const
    if b0 == 116 && b1 == 114 && b2 == 97 && b3 == 105 && b4 == 116 { return TokenKind.KwTrait; } // trait
    if b0 == 97 && b1 == 115 && b2 == 121 && b3 == 110 && b4 == 99 { return TokenKind.KwAsync; } // async
    if b0 == 97 && b1 == 119 && b2 == 97 && b3 == 105 && b4 == 116 { return TokenKind.KwAwait; } // await
    return TokenKind.Ident;
  }

  // 6
  if n == 6 {
    let b0: i32 = text_byte_at(text, start + 0);
    let b1: i32 = text_byte_at(text, start + 1);
    let b2: i32 = text_byte_at(text, start + 2);
    let b3: i32 = text_byte_at(text, start + 3);
    let b4: i32 = text_byte_at(text, start + 4);
    let b5: i32 = text_byte_at(text, start + 5);
    if b0 == 114 && b1 == 101 && b2 == 116 && b3 == 117 && b4 == 114 && b5 == 110 { return TokenKind.KwReturn; } // return
    if b0 == 115 && b1 == 116 && b2 == 114 && b3 == 117 && b4 == 99 && b5 == 116 { return TokenKind.KwStruct; } // struct
    if b0 == 105 && b1 == 109 && b2 == 112 && b3 == 111 && b4 == 114 && b5 == 116 { return TokenKind.KwImport; } // import
    if b0 == 115 && b1 == 116 && b2 == 97 && b3 == 116 && b4 == 105 && b5 == 99 { return TokenKind.KwStatic; } // static
    return TokenKind.Ident;
  }

  // 8
  if n == 8 {
    let b0: i32 = text_byte_at(text, start + 0);
    let b1: i32 = text_byte_at(text, start + 1);
    let b2: i32 = text_byte_at(text, start + 2);
    let b3: i32 = text_byte_at(text, start + 3);
    let b4: i32 = text_byte_at(text, start + 4);
    let b5: i32 = text_byte_at(text, start + 5);
    let b6: i32 = text_byte_at(text, start + 6);
    let b7: i32 = text_byte_at(text, start + 7);
    if b0 == 99 && b1 == 111 && b2 == 110 && b3 == 116 && b4 == 105 && b5 == 110 && b6 == 117 && b7 == 101 {
      return TokenKind.KwContinue; // continue
    }
    if b0 == 99 && b1 == 111 && b2 == 109 && b3 == 112 && b4 == 116 && b5 == 105 && b6 == 109 && b7 == 101 {
      return TokenKind.KwComptime; // comptime
    }
    return TokenKind.Ident;
  }

  return TokenKind.Ident;
}

pub fn lex_text(text: String) -> LexResult {
  let mut i: i32 = 0;
  let n: i32 = text.len();
  let mut line: i32 = 1;
  let mut col: i32 = 1;
  let mut toks: Vec[Token] = Vec();

  while i < n {
    let b: i32 = text_byte_at(text, i);

    // whitespace
    if is_space(b) {
      i = i + 1;
      if b == 10 {
        line = line + 1;
        col = 1;
      } else if !is_utf8_cont_byte(b) {
        col = col + 1;
      }
      continue;
    }

    // line comment: //
    if b == 47 {
      if (i + 1) < n {
        if text_byte_at(text, i + 1) == 47 {
          i = i + 2;
          col = col + 2;
          while i < n {
            let cb: i32 = text_byte_at(text, i);
            if cb == 10 {
              break;
            }
            i = i + 1;
            if !is_utf8_cont_byte(cb) { col = col + 1; }
          }
          continue;
        }
      }
    }

    let start: i32 = i;
    let start_line: i32 = line;
    let start_col: i32 = col;

    // identifier / keyword
    if is_alpha(b) {
      i = i + 1;
      col = col + 1;
      while i < n {
        if !is_alnum(text_byte_at(text, i)) {
          break;
        }
        i = i + 1;
        col = col + 1;
      }
      let k: TokenKind = kw_kind(text, start, i);
      toks.push(token_make(k, start, i, start_line, start_col));
      continue;
    }

    // number literal: int or float.
    // float forms:
    // - digits '.' digits
    // - digits [ '.' digits ] [e|E [+-]? digits]
    // - any of the above with suffix f32/f64
    if is_digit(b) {
      let mut is_float: bool = false;
      i = i + 1;
      col = col + 1;
      while i < n {
        if !is_digit(text_byte_at(text, i)) {
          break;
        }
        i = i + 1;
        col = col + 1;
      }

      // Fractional part requires at least one digit after dot.
      if i < n && text_byte_at(text, i) == 46 && (i + 1) < n && is_digit(text_byte_at(text, i + 1)) {
        is_float = true;
        i = i + 1; // '.'
        col = col + 1;
        while i < n {
          if !is_digit(text_byte_at(text, i)) { break; }
          i = i + 1;
          col = col + 1;
        }
      }

      // Optional scientific exponent.
      if i < n && (text_byte_at(text, i) == 101 || text_byte_at(text, i) == 69) { // e/E
        let mut j: i32 = i + 1;
        if j < n && (text_byte_at(text, j) == 43 || text_byte_at(text, j) == 45) { j = j + 1; } // +/-
        if j < n && is_digit(text_byte_at(text, j)) {
          is_float = true;
          col = col + (j - i);
          i = j + 1;
          col = col + 1;
          while i < n {
            if !is_digit(text_byte_at(text, i)) { break; }
            i = i + 1;
            col = col + 1;
          }
        }
      }

      // Optional explicit float suffix.
      if (i + 2) < n && text_byte_at(text, i) == 102 { // 'f'
        let b1: i32 = text_byte_at(text, i + 1);
        let b2: i32 = text_byte_at(text, i + 2);
        if (b1 == 51 && b2 == 50) || (b1 == 54 && b2 == 52) { // 32 / 64
          is_float = true;
          i = i + 3;
          col = col + 3;
        }
      }

      let kind: TokenKind = if is_float { TokenKind.Float } else { TokenKind.Int };
      toks.push(token_make(kind, start, i, start_line, start_col));
      continue;
    }

    // string literal: "..." or multiline """..."""
    if b == 34 { // '"'
      // Multiline triple-quoted string.
      if (i + 2) < n && text_byte_at(text, i + 1) == 34 && text_byte_at(text, i + 2) == 34 {
        i = i + 3;
        col = col + 3;
        let mut closed3: bool = false;
        while (i + 2) < n {
          if text_byte_at(text, i) == 34 && text_byte_at(text, i + 1) == 34 && text_byte_at(text, i + 2) == 34 {
            i = i + 3;
            col = col + 3;
            toks.push(token_make(TokenKind.Str, start, i, start_line, start_col));
            closed3 = true;
            break;
          }
          let cb: i32 = text_byte_at(text, i);
          i = i + 1;
          if cb == 10 {
            line = line + 1;
            col = 1;
          } else if !is_utf8_cont_byte(cb) {
            col = col + 1;
          }
        }
        if !closed3 {
          return LexResult { tokens: toks, err: LexError.UnterminatedString(start) };
        }
        continue;
      }

      i = i + 1;
      col = col + 1;
      let mut closed: bool = false;
      while i < n {
        let c: i32 = text_byte_at(text, i);
        if c == 34 { // closing '"'
          i = i + 1;
          col = col + 1;
          toks.push(token_make(TokenKind.Str, start, i, start_line, start_col));
          closed = true;
          break;
        }
        if c == 92 { // '\\' escape, skip next char if present
          i = i + 1;
          col = col + 1;
          if i < n {
            let c2: i32 = text_byte_at(text, i);
            i = i + 1;
            if c2 == 10 {
              line = line + 1;
              col = 1;
            } else if !is_utf8_cont_byte(c2) {
              col = col + 1;
            }
          }
          continue;
        }
        i = i + 1;
        if c == 10 {
          line = line + 1;
          col = 1;
        } else if !is_utf8_cont_byte(c) {
          col = col + 1;
        }
      }
      if !closed {
        return LexResult { tokens: toks, err: LexError.UnterminatedString(start) };
      }
      continue;
    }

    // char literal: `'a'`, `'\n'`, `'\''`, `'ä½ '`
    //
    // We only treat it as a char token when the quote pair encloses exactly one
    // scalar or one escape sequence. Otherwise fallback to Tick token, so
    // lifetime-like syntax (`'static`) keeps existing behavior.
    if b == 39 { // '\''
      let mut end_char: i32 = -1;
      if (i + 1) < n {
        let b1: i32 = text_byte_at(text, i + 1);
        if b1 == 92 { // escape form: '\'' '\n' ...
          if (i + 3) < n && text_byte_at(text, i + 3) == 39 {
            end_char = i + 4;
          }
        } else if b1 != 39 && b1 != 10 && b1 != 13 {
          let scalar_end: i32 = utf8_next_scalar_end(text, i + 1, n);
          if scalar_end < n && text_byte_at(text, scalar_end) == 39 {
            end_char = scalar_end + 1;
          }
        }
      }
      if end_char != -1 {
        let mut j: i32 = i;
        while j < end_char {
          let cb: i32 = text_byte_at(text, j);
          if cb == 10 {
            line = line + 1;
            col = 1;
          } else if !is_utf8_cont_byte(cb) {
            col = col + 1;
          }
          j = j + 1;
        }
        toks.push(token_make(TokenKind.Char, start, end_char, start_line, start_col));
        i = end_char;
        continue;
      }
      i = i + 1;
      col = col + 1;
      toks.push(token_make(TokenKind.Tick, start, i, start_line, start_col));
      continue;
    }

    // punctuators / operators (maximal munch)
    // 3-char ops
    if (i + 2) < n {
      let b1: i32 = text_byte_at(text, i + 1);
      let b2: i32 = text_byte_at(text, i + 2);
      // ...
      if b == 46 && b1 == 46 && b2 == 46 {
        i = i + 3;
        col = col + 3;
        toks.push(token_make(TokenKind.DotDotDot, start, i, start_line, start_col));
        continue;
      }
      // ..=
      if b == 46 && b1 == 46 && b2 == 61 {
        i = i + 3;
        col = col + 3;
        toks.push(token_make(TokenKind.DotDotEq, start, i, start_line, start_col));
        continue;
      }
      // <<=
      if b == 60 && b1 == 60 && b2 == 61 {
        i = i + 3;
        col = col + 3;
        toks.push(token_make(TokenKind.LtLtEq, start, i, start_line, start_col));
        continue;
      }
      // >>=
      if b == 62 && b1 == 62 && b2 == 61 {
        i = i + 3;
        col = col + 3;
        toks.push(token_make(TokenKind.GtGtEq, start, i, start_line, start_col));
        continue;
      }
    }

    // 2-char ops
    if (i + 1) < n {
      let b1: i32 = text_byte_at(text, i + 1);
      // ->
      if b == 45 && b1 == 62 {
        i = i + 2;
        col = col + 2;
        toks.push(token_make(TokenKind.Arrow, start, i, start_line, start_col));
        continue;
      }
      // ..
      if b == 46 && b1 == 46 {
        i = i + 2;
        col = col + 2;
        toks.push(token_make(TokenKind.DotDot, start, i, start_line, start_col));
        continue;
      }
      // =>
      if b == 61 && b1 == 62 {
        i = i + 2;
        col = col + 2;
        toks.push(token_make(TokenKind.FatArrow, start, i, start_line, start_col));
        continue;
      }
      // +=
      if b == 43 && b1 == 61 {
        i = i + 2;
        col = col + 2;
        toks.push(token_make(TokenKind.PlusEq, start, i, start_line, start_col));
        continue;
      }
      // -=
      if b == 45 && b1 == 61 {
        i = i + 2;
        col = col + 2;
        toks.push(token_make(TokenKind.MinusEq, start, i, start_line, start_col));
        continue;
      }
      // *=
      if b == 42 && b1 == 61 {
        i = i + 2;
        col = col + 2;
        toks.push(token_make(TokenKind.StarEq, start, i, start_line, start_col));
        continue;
      }
      // /=
      if b == 47 && b1 == 61 {
        i = i + 2;
        col = col + 2;
        toks.push(token_make(TokenKind.SlashEq, start, i, start_line, start_col));
        continue;
      }
      // %=
      if b == 37 && b1 == 61 {
        i = i + 2;
        col = col + 2;
        toks.push(token_make(TokenKind.PercentEq, start, i, start_line, start_col));
        continue;
      }
      // ==
      if b == 61 && b1 == 61 {
        i = i + 2;
        col = col + 2;
        toks.push(token_make(TokenKind.EqEq, start, i, start_line, start_col));
        continue;
      }
      // !=
      if b == 33 && b1 == 61 {
        i = i + 2;
        col = col + 2;
        toks.push(token_make(TokenKind.Ne, start, i, start_line, start_col));
        continue;
      }
      // <=
      if b == 60 && b1 == 61 {
        i = i + 2;
        col = col + 2;
        toks.push(token_make(TokenKind.Le, start, i, start_line, start_col));
        continue;
      }
      // <<
      if b == 60 && b1 == 60 {
        i = i + 2;
        col = col + 2;
        toks.push(token_make(TokenKind.LtLt, start, i, start_line, start_col));
        continue;
      }
      // >=
      if b == 62 && b1 == 61 {
        i = i + 2;
        col = col + 2;
        toks.push(token_make(TokenKind.Ge, start, i, start_line, start_col));
        continue;
      }
      // >>
      if b == 62 && b1 == 62 {
        i = i + 2;
        col = col + 2;
        toks.push(token_make(TokenKind.GtGt, start, i, start_line, start_col));
        continue;
      }
      // &&
      if b == 38 && b1 == 38 {
        i = i + 2;
        col = col + 2;
        toks.push(token_make(TokenKind.AndAnd, start, i, start_line, start_col));
        continue;
      }
      // &=
      if b == 38 && b1 == 61 {
        i = i + 2;
        col = col + 2;
        toks.push(token_make(TokenKind.AmpEq, start, i, start_line, start_col));
        continue;
      }
      // ||
      if b == 124 && b1 == 124 {
        i = i + 2;
        col = col + 2;
        toks.push(token_make(TokenKind.OrOr, start, i, start_line, start_col));
        continue;
      }
      // |=
      if b == 124 && b1 == 61 {
        i = i + 2;
        col = col + 2;
        toks.push(token_make(TokenKind.PipeEq, start, i, start_line, start_col));
        continue;
      }
      // ^=
      if b == 94 && b1 == 61 {
        i = i + 2;
        col = col + 2;
        toks.push(token_make(TokenKind.CaretEq, start, i, start_line, start_col));
        continue;
      }
    }

    // 1-char tokens
    if b == 40 { i = i + 1; col = col + 1; toks.push(token_make(TokenKind.LParen, start, i, start_line, start_col)); continue; }   // (
    if b == 41 { i = i + 1; col = col + 1; toks.push(token_make(TokenKind.RParen, start, i, start_line, start_col)); continue; }   // )
    if b == 123 { i = i + 1; col = col + 1; toks.push(token_make(TokenKind.LBrace, start, i, start_line, start_col)); continue; }  // {
    if b == 125 { i = i + 1; col = col + 1; toks.push(token_make(TokenKind.RBrace, start, i, start_line, start_col)); continue; }  // }
    if b == 91 { i = i + 1; col = col + 1; toks.push(token_make(TokenKind.LBracket, start, i, start_line, start_col)); continue; } // [
    if b == 93 { i = i + 1; col = col + 1; toks.push(token_make(TokenKind.RBracket, start, i, start_line, start_col)); continue; } // ]
    if b == 44 { i = i + 1; col = col + 1; toks.push(token_make(TokenKind.Comma, start, i, start_line, start_col)); continue; }    // ,
    if b == 59 { i = i + 1; col = col + 1; toks.push(token_make(TokenKind.Semicolon, start, i, start_line, start_col)); continue; } // ;
    if b == 46 { i = i + 1; col = col + 1; toks.push(token_make(TokenKind.Dot, start, i, start_line, start_col)); continue; }      // .
    if b == 58 { i = i + 1; col = col + 1; toks.push(token_make(TokenKind.Colon, start, i, start_line, start_col)); continue; }    // :
    if b == 64 { i = i + 1; col = col + 1; toks.push(token_make(TokenKind.At, start, i, start_line, start_col)); continue; }       // @
    if b == 36 { i = i + 1; col = col + 1; toks.push(token_make(TokenKind.Dollar, start, i, start_line, start_col)); continue; }   // $
    if b == 39 { i = i + 1; col = col + 1; toks.push(token_make(TokenKind.Tick, start, i, start_line, start_col)); continue; }     // '
    if b == 63 { i = i + 1; col = col + 1; toks.push(token_make(TokenKind.Question, start, i, start_line, start_col)); continue; } // ?

    if b == 61 { i = i + 1; col = col + 1; toks.push(token_make(TokenKind.Eq, start, i, start_line, start_col)); continue; }      // =
    if b == 60 { i = i + 1; col = col + 1; toks.push(token_make(TokenKind.Lt, start, i, start_line, start_col)); continue; }      // <
    if b == 62 { i = i + 1; col = col + 1; toks.push(token_make(TokenKind.Gt, start, i, start_line, start_col)); continue; }      // >
    if b == 33 { i = i + 1; col = col + 1; toks.push(token_make(TokenKind.Not, start, i, start_line, start_col)); continue; }     // !

    if b == 43 { i = i + 1; col = col + 1; toks.push(token_make(TokenKind.Plus, start, i, start_line, start_col)); continue; }    // +
    if b == 45 { i = i + 1; col = col + 1; toks.push(token_make(TokenKind.Minus, start, i, start_line, start_col)); continue; }   // -
    if b == 42 { i = i + 1; col = col + 1; toks.push(token_make(TokenKind.Star, start, i, start_line, start_col)); continue; }    // *
    if b == 47 { i = i + 1; col = col + 1; toks.push(token_make(TokenKind.Slash, start, i, start_line, start_col)); continue; }   // /
    if b == 37 { i = i + 1; col = col + 1; toks.push(token_make(TokenKind.Percent, start, i, start_line, start_col)); continue; } // %
    if b == 38 { i = i + 1; col = col + 1; toks.push(token_make(TokenKind.Amp, start, i, start_line, start_col)); continue; }     // &
    if b == 124 { i = i + 1; col = col + 1; toks.push(token_make(TokenKind.Pipe, start, i, start_line, start_col)); continue; }   // |
    if b == 94 { i = i + 1; col = col + 1; toks.push(token_make(TokenKind.Caret, start, i, start_line, start_col)); continue; }   // ^

    return LexResult { tokens: toks, err: LexError.UnexpectedChar(i) };
  }

  toks.push(token_make(TokenKind.Eof, n, n, line, col));
  return LexResult { tokens: toks, err: LexError.None };
}
