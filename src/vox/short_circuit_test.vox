import "std/testing" as t

fn rhs_panic() -> bool {
  panic("rhs executed");
  return true;
}

fn test_short_circuit_and_and() -> () {
  let x: bool = false && rhs_panic();
  t.assert_eq(x, false);
}

fn test_short_circuit_or_or() -> () {
  let x: bool = true || rhs_panic();
  t.assert_eq(x, true);
}
