import "std/testing" as t
import "std/async" as a

// This is an async test function; the test runner must be able to execute it by
// generating a sync wrapper during compilation.
async fn test_async_test_runner_smoke() -> () {
  let p: a.Poll[i32] = a.Poll.Ready(1);
  let x: i32 = p.await;
  t.assert_eq(x, 1);
}
