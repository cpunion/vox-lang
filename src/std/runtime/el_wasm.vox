// std/runtime/el_wasm: stub poller for wasm (single-threaded, no I/O mux).

@build(wasm)
import "std/sys" as sys

fn el_init_poller() -> () {
  return;
}

fn el_wake_poller() -> () {
  sys.yield_now();
  return;
}

fn el_wait_poller(timeout_ms: i32) -> () {
  timeout_ms;
  sys.yield_now();
  return;
}
