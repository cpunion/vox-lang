// std/time: minimal wall-clock helpers for tooling/test reporting.

pub fn now_ns() -> i64 { return __now_ns(); }

// Stable scheduling-yield API for std/runtime hooks.
// Phase-1 keeps behavior conservative (no-op) to preserve rolling-bootstrap
// compatibility. A later phase can wire this to platform yield intrinsic.
pub fn yield_now() -> () { return; }
