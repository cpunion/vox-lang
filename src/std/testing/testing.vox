// stage1 std/testing: implemented in Vox, layered on std/prelude.

import "std/prelude" as prelude

pub fn assert(cond: bool) -> () { prelude.assert(cond); }

pub fn assert_with(cond: bool, msg: String) -> () { prelude.assert_with(cond, msg); }

pub fn fail(msg: String) -> () { prelude.fail(msg); }

pub fn assert_eq[T: prelude.Eq](a: T, b: T) -> () { prelude.assert_eq(a, b); }

pub fn assert_ne[T: prelude.Eq](a: T, b: T) -> () { prelude.assert_ne(a, b); }

pub fn assert_lt[T: prelude.Ord](a: T, b: T) -> () { prelude.assert_lt(a, b); }

pub fn assert_le[T: prelude.Ord](a: T, b: T) -> () { prelude.assert_le(a, b); }

pub fn assert_gt[T: prelude.Ord](a: T, b: T) -> () { prelude.assert_gt(a, b); }

pub fn assert_ge[T: prelude.Ord](a: T, b: T) -> () { prelude.assert_ge(a, b); }
