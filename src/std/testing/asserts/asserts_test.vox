import "std/testing" as t
import "std/testing/asserts" as a

fn test_asserts_bool_helpers_smoke() -> () {
  a.is_true(true);
  a.is_false(false);
  t.assert(true);
}

fn test_asserts_string_helpers_smoke() -> () {
  a.str_contains("hello world", "lo wo");
  a.str_not_contains("hello world", "xyz");
  a.str_starts_with("hello world", "hello");
  a.str_ends_with("hello world", "world");
  a.str_empty("");
  a.str_not_empty("x");
  t.assert(true);
}
