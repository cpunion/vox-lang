import "std/sys" as sys

pub fn print(msg: String) -> () {
  let total: i32 = msg.len();
  let mut off: i32 = 0;
  while off < total {
    let chunk: String = msg.slice(off, total);
    let wrote: isize = sys.write(1, chunk as const rawptr, chunk.len() as usize);
    if wrote <= 0 { return; }
    let step: i32 = wrote as i32;
    if step >= chunk.len() {
      off = total;
    } else {
      off = off + step;
    }
  }
}
