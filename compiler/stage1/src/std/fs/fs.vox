// std/fs: minimal filesystem primitives for stage1 toolchain.

pub fn read_to_string(path: String) -> String { return __read_file(path); }

pub fn write_string(path: String, text: String) -> () { __write_file(path, text); }

pub fn walk_vox_files(root: String) -> Vec[String] { return __walk_vox_files(root); }
