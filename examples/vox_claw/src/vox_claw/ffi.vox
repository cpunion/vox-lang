@ffi_import("c", "strcmp")
fn c_strcmp_raw(a: String, b: String) -> i32;

pub fn ffi_eq(a: String, b: String) -> bool {
  return c_strcmp_raw(a, b) == 0;
}

pub fn ffi_order(a: String, b: String) -> i32 {
  return c_strcmp_raw(a, b);
}

pub fn ffi_prefix_score(cmd: String) -> i32 {
  if cmd.len() >= 2 && ffi_eq(cmd.slice(0, 2), "rm") { return 10; }
  if cmd.len() >= 3 && ffi_eq(cmd.slice(0, 3), "git") { return 3; }
  return 1;
}
