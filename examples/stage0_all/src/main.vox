// stage0 demo: named imports + pub nominal types + enum/match + Vec[T].

import { add, Point, Opt } from "utils/math"

fn main() -> i32 {
  let mut v: Vec[i32] = Vec();
  v.push(add(40, 1));
  v.push(1);

  let o: Opt = Opt.Some(v.get(0));
  let x: i32 = match o {
    Opt.Some(n) => n,
    Opt.None => 0,
  };

  let p: Point = Point { x: x, y: v.len() };
  return p.x + p.y + v.get(1); // 41 + 2 + 1 = 44
}

